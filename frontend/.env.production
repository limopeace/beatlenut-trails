# Production Environment Variables for Vercel

# API Configuration - Update these after backend deployment
NEXT_PUBLIC_API_URL=https://your-backend-url.vercel.app
NEXT_PUBLIC_DOMAIN=https://your-app-name.vercel.app

# Third-party Services
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Environment
NODE_ENV=production

# Security - Set in Vercel dashboard
NEXTAUTH_SECRET=your_production_nextauth_secret_here
NEXTAUTH_URL=https://your-app-name.vercel.app

# Always use the real API in production
NEXT_PUBLIC_USE_REAL_API=true

# Disable telemetry
NEXT_TELEMETRY_DISABLED=1